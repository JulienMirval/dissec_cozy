{
  "name": "DISSEC-COZY",
  "slug": "dissecozy",
  "icon": "icon.svg",
  "categories": [],
  "version": "0.1.0",
  "licence": "AGPL-3.0",
  "editor": "",
  "source": "https://github.com/JulienMirval/dissec_cozy.git@build",
  "developer": {
    "name": "JulienMirval",
    "url": ""
  },
  "routes": {
    "/": {
      "folder": "/",
      "index": "index.html",
      "public": false
    }, 
    "/shares": {
      "folder": "/shares",
      "public": true
    }
  },
  "services": {
    "contribution": {
      "type": "node",
      "file": "services/contribution/dissecozy.js",
      "trigger": "@webhook"
    },
    "aggregation": {
      "type": "node",
      "file": "services/aggregation/dissecozy.js",
      "trigger": "@webhook"
    }
  },
  "permissions": {
    "apps": {
      "description": "Required by the cozy-bar to display the icons of the apps",
      "type": "io.cozy.apps",
      "verbs": ["GET"]
    },
    "settings": {
      "description": "Required by the cozy-bar to display Claudy and know which applications are coming soon",
      "type": "io.cozy.settings",
      "verbs": ["GET"]
    },
    "bank.operations": {
      "description": "Used to manage your bank operations",
      "type": "io.cozy.bank.operations",
      "verbs": ["GET", "POST", "PUT", "DELETE"]
    },
    "jobs": {
      "description": "Required to run the services",
      "type": "io.cozy.jobs",
      "verbs": ["ALL"]
    },
    "triggers": {
      "description": "Required to run the services",
      "type": "io.cozy.triggers",
      "verbs": ["ALL"]
    },
    "dissec.shares": {
      "description": "Intermediate results used for privately aggregating data",
      "type": "io.cozy.dissec.shares",
      "verbs": ["ALL"]
    },
    "dissec.models": {
      "description": "Usable classification models",
      "type": "io.cozy.dissec.models",
      "verbs": ["ALL"]
    },
    "dissecozy": {
      "description": "Required to enable asynchronous computations",
      "type": "io.cozy.triggers",
      "verbs": ["POST"],
      "selector": "worker",
      "values": ["service"]
    }
  }
}
